{
  "$schema": "https://turborepo.org/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**/*", "tsconfig.build.tsbuildinfo"]
    },
    "cleanup": {
      "cache": false
    },
    "docs": {
      "dependsOn": ["^build"],
      "outputs": ["docs/**/*"]
    },
    "fix": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", "../../eslint.config.js", "../../prettier.config.js"],
      "outputs": [".eslintcache", "tsconfig.buildinfo"]
    },
    "lint": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", "../../eslint.config.js"],
      "outputs": [".eslintcache"]
    },
    "lint:fix": {
      "inputs": ["$TURBO_DEFAULT$", "../../eslint.config.js"],
      "outputs": [".eslintchache"]
    },
    "format": {
      "inputs": ["$TURBO_DEFAULT$", "../../prettier.config.js"],
      "outputs": []
    },
    "format:fix": {
      "inputs": ["$TURBO_DEFAULT$", "../../prettier.config.js"],
      "outputs": []
    },
    "qa": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", "../../eslint.config.js", "../../prettier.config.js"],
      "outputs": [".eslintcache", "tsconfig.buildinfo"]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "test:coverage": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**/*"]
    },
    "ts": {
      "dependsOn": ["^build"],
      "outputs": ["tsconfig.tsbuildinfo"]
    }
  }
}
