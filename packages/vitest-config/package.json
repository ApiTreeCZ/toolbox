{
  "name": "@apitree.cz/vitest-config",
  "version": "0.8.6",
  "description": "Vitest configuration for ApiTree projects.",
  "keywords": [
    "apitree",
    "vitest",
    "config",
    "nodejs",
    "react",
    "nestjs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ApiTreeCZ/toolbox",
    "directory": "packages/vitest-config"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./base": {
      "types": "./dist/base.d.ts",
      "import": "./dist/base.js"
    },
    "./constants": {
      "types": "./dist/constants.d.ts",
      "import": "./dist/constants.js"
    },
    "./nestjs": {
      "types": "./dist/nestjs.d.ts",
      "import": "./dist/nestjs.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "del dist tsconfig.build.tsbuildinfo",
    "build": "tsc --build tsconfig.build.json",
    "cleanup": "del .eslintcache .turbo tsconfig.tsbuildinfo tsconfig.build.tsbuildinfo dist node_modules",
    "fix": "turbo run ts format:fix lint:fix --no-daemon",
    "format": "prettier --check \"./**/*.{js,json,md,ts}\"",
    "format:fix": "pnpm run format --write",
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "qa": "turbo run ts format lint --no-daemon",
    "ts": "tsc --build tsconfig.json"
  },
  "dependencies": {
    "@swc/core": "^1.15.8",
    "@vitejs/plugin-react": "^5.1.2",
    "unplugin-swc": "^1.5.9",
    "vite": "^7.3.1"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.16",
    "jsdom": "^28.0.0",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "@vitest/coverage-v8": "^4.0.15",
    "jsdom": "^27.0.0 || ^28.0.0",
    "vitest": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "@vitest/coverage-v8": {
      "optional": true
    },
    "jsdom": {
      "optional": true
    }
  }
}
